  "cross_validation_rules": {
    "wbc_differential_sum": {
      "description": "WBC Differential 합계 검증",
      "fields": ["NEU", "LYM", "MONO", "EOS", "BASO"],
      "expected_sum": 100,
      "tolerance": 5
    },
    "ag_ratio": {
      "description": "A/G Ratio 검증",
      "formula": "ALB / GLOB",
      "expected_range": [0.5, 2.0]
    },
    "tp_calculation": {
      "description": "TP = ALB + GLOB 검증",
      "fields": ["TP", "ALB", "GLOB"],
      "tolerance": 0.5
    }
  }
